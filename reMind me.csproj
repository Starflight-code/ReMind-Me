<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>reMind_me</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="build\**" />
    <Compile Remove="CMakeFiles\**" />
    <EmbeddedResource Remove="build\**" />
    <EmbeddedResource Remove="CMakeFiles\**" />
    <None Remove="build\**" />
    <None Remove="CMakeFiles\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="CMakeCache.txt" />
    <None Remove="CMakeLists.txt" />
  </ItemGroup>

</Project>
